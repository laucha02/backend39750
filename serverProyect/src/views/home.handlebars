<div>
  <h1 class="title">Listado de productos</h1>
  <table>
    <thead>
      <tr>
        <th>Id</th>
        <th>Título</th>
        <th>Descripción</th>
        <th>Precio</th>
        <th>Imagen</th>
        <th>Stock</th>
        <th>Código</th>
      </tr>
    </thead>
    <tbody>
      {{#each listaProductos}}
        <tr>
          <td>{{this.id}}</td>
          <td>{{this.title}}</td>
          <td>{{this.description}}</td>
          <td>{{this.price}}</td>
          <td>{{this.thumbnails}}</td>
          <td>{{this.stock}}</td>
          <td>{{this.code}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>